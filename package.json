{
  "name": "html5-skin",
  "version": "0.0.1",
  "description": "HTML5 skin over an Ooyala video player",
  "engines": {
    "node": ">=8.11.3"
  },
  "keywords": [
    "html5",
    "react",
    "ooyala",
    "video",
    "player"
  ],
  "main": "html5-skin.js",
  "scripts": {
    "test": "jest",
    "eslint": "git diff --cached --name-only --diff-filter=ACMRTUB | grep .js$ | xargs ./node_modules/.bin/eslint --",
    "eslint:fix": "git diff --cached --name-only --diff-filter=ACMRTUB | grep .js$ | xargs ./node_modules/.bin/eslint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ooyala/html5-skin.git"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "create-react-class": "^15.6.3",
    "deepmerge": "^1.4.0",
    "lodash.debounce": "4.0.3",
    "lodash.values": "4.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scrollbar": "^0.5.4",
    "screenfull": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0-beta.51",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-polyfill": "^6.26.0",
    "babelify": "^9.0.0",
    "browserify": "^16.2.2",
    "bulk-require": "^1.0.0",
    "bulkify": "^1.4.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-require-jsdoc-except": "^1.2.0",
    "git-rev": "0.2.1",
    "graceful-fs": "^4.1.11",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.6.1",
    "gulp-open": "^3.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-shell": "^0.6.5",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "gulp-util": "^3.0.8",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jquery": "^3.3.1",
    "jsdoc": "^3.5.5",
    "jsdoc-jsx": "^0.1.0",
    "reactify": "1.1.1",
    "sinon": "^6.3.4",
    "underscore": "^1.9.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^1.1.2",
    "watchify": "^3.11.0"
  },
  "author": "Ooyala",
  "contributors": [
    "Aldi Gunawan <agunawan@ooyala.com>",
    "Timothy Chang <tchang@ooyala.com>",
    "Jeoffrey Mendez <jmendez@ooyala.com>",
    "Dustin Preuss <dustin@ooyala.com"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ooyala/html5-skin/issues"
  },
  "homepage": "https://github.com/ooyala/html5-skin",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/jest/setup.js",
    "modulePathIgnorePatterns": [
      "/build/",
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "react",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react-dom"
    ],
    "testMatch": [
      "**/tests/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/tests/controller-test.js"
    ],
    "collectCoverage": true
  }
}
